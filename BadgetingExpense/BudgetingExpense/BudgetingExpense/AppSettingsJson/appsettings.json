{
  "ConnectionStrings": {
    "default": "Server=(localdb)\\mssqllocaldb;Database=BudgetingExpenseTracker;Integrated Security=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  //JWT Token Configuration
  "Jwt": {
    "Key": "",
    "Issuer": "AuthenticationServer",
    "Audience": "ApiUsers",
    "ExpiryMinutes": "60"
  },
  //Define The Frequency Of The Recorded Data For The Forecast
  "ConfigureForcastCounts": {
    "IncomeForecastCount": 2,
    "ExpenseForecastCount": 2
  },
  //Define Page Size For Fetch All Records
  "PageSize": {
    "rows": 2
  }
}
